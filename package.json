{
  "name": "js2coffee",
  "version": "0.2.4-DEV",
  "description": "JavaScript to CoffeeScript compiler",
  "homepage": "http://ricostacruz.com/js2coffee",
  "keywords": [
    "javascript",
    "coffeescript",
    "language",
    "compiler"
  ],
  "author": "Rico Sta. Cruz <hi@ricostacruz.com> (http://ricostacruz.com)",
  "maintainers": [
    "Rico Sta. Cruz <hi@ricostacruz.com> (http://ricostacruz.com)",
    "Benjamin Lupton <b@lupton.cc> (https://github.com/balupton)"
  ],
  "contributors": [
    "Rico Sta. Cruz <hi@ricostacruz.com> (http://ricostacruz.com)",
    "Benjamin Lupton <b@lupton.cc> (https://github.com/balupton)",
    "Anton Wilhelm <timaschew@gmail.com> (https://github.com/timaschew)"
  ],
  "bugs": {
    "url": "https://github.com/rstacruz/js2coffee/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rstacruz/js2coffee.git"
  },
  "engines": {
    "node": ">=0.4.0"
  },
  "dependencies": {
    "underscore": "~1.5.1",
    "coffee-script": "~1.6.2",
    "file": "~0.2.1",
    "nopt": "~2.1.1"
  },
  "devDependencies": {
    "glob": "~3.2.1",
    "docpad": ">=6.21.5 <7",
    "docpad-plugin-eco": "2.x",
    "docpad-plugin-coffeescript": "2.x",
    "docpad-plugin-uglify": "2.x",
    "joe": "~1.3.0",
    "joe-reporter-console": "~1.2.1",
    "ansidiff": "~1.0.0",
    "browserify": "~3.18.0"
  },
  "directories": {
    "lib": "./out/lib"
  },
  "bin": {
    "js2coffee": "./out/bin/js2coffee"
  },
  "scripts": {
    "preinstall": "node ./cyclic.js",
    "watch": "./node_modules/.bin/cake watch",
    "compile": "./node_modules/.bin/cake compile",
    "test": "./node_modules/.bin/cake test"
  },
  "main": "./out/lib/js2coffee"
}
